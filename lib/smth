                      padding: const EdgeInsets.only(top: 10),
                      child: Container(
                        decoration: BoxDecoration(
                            boxShadow: [
                              BoxShadow(
                                color: Color(0xFF285260).withOpacity(1),
                                spreadRadius: 5, // Spread value
                                blurRadius: 20, // Blur value
                                offset: Offset(0, 0),
                              )
                            ]
                        ),
                        child: ElevatedButton(onPressed: (){
                          if(_pickedimage==null){
                            ScaffoldMessenger.of(context).showSnackBar(
                                const SnackBar(content: Text('please choise personal photo',
                                  style: TextStyle(color: Colors.black),),
                                  backgroundColor: Colors.white,));
                            return;

                          }
                          if(selectedRole==null){
                            ScaffoldMessenger.of(context).showSnackBar(
                                const SnackBar(content:Text('please choise role',style: TextStyle(color: Colors.black),)
                                  ,backgroundColor: Colors.white, ));
                            return;
                          }
                          if(_formKey.currentState!.validate()) {
                            // Navigator.push(
                            //     context, MaterialPageRoute(builder: (context) {
                            //   return NextIformation();
                            // }));
                          }
                        },
                          style: ElevatedButton.styleFrom(


                            backgroundColor: const Color(0XFF52878b),
                            padding: const EdgeInsets.symmetric(horizontal: 50, vertical: 20),
                            shape: RoundedRectangleBorder(borderRadius: BorderRadius.circular(15)),
                          ),
                          child: const Text('SignUp', style: TextStyle(fontSize: 18, color: Colors.white)),
                        ),
                      ),
                    ),


                  ],

                ),
              ),
            ),
          ),
        )
    );




  }

